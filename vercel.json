{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/assets/.*",
      "dest": "/$0",
      "continue": true
    },
    {
      "src": "^/index\\.html$",
      "dest": "/index.html"
    },
    {
      "src": "^/.*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
